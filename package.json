{
  "name": "security-dependency-tool",
  "version": "1.0.0",
  "description": "A comprehensive web-based tool for managing security vulnerabilities identified by Wiz Security across multiple repositories",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd src/frontend && npm run dev",
    "dev:backend": "cd src/backend && npm run dev",
    "build": "cd src/frontend && npm run build",
    "start": "cd src/backend && npm start",
    "install:all": "npm install && cd src/frontend && npm install && cd ../backend && npm install",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx"
  },
  "keywords": [
    "security",
    "dependency-management",
    "vulnerability",
    "github",
    "jira",
    "wiz"
  ],
  "author": "Security Team",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  },
  "workspaces": [
    "src/frontend",
    "src/backend"
  ],
  "dependencies": {
    "@octokit/plugin-retry": "^8.0.1",
    "@octokit/rest": "^22.0.0"
  }
}
